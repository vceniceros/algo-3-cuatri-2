d' stamp: 'SV 4/15/2024 23:22'!serieFibonacciOrden: n 	| b a |	(n = 1) ifTrue: [^0].	(n <= 3) ifTrue: [^1].	a := b := 1.	(n - 2) timesRepeat: [a := b + (b := a)].	^b.! !!AzadaNoTemplada methodsFor: 'as yet unclassified' stamp: 'SV 4/15/2024 23:24'!usar 		| aRestar |		usos := usos + 1.		aRestar := self serieFibonacciOrden: usos.		durabilidad := durabilidad - aRestar.		(durabilidad <= 0) ifTrue: [ durabilidad := 0 ]. 		^durabilidad ! !Azada subclass: #AzadaTemplada	instanceVariableNames: ''	classVariableNames: ''	package: 'Herramientas'!!AzadaTemplada methodsFor: 'as yet unclassified' stamp: 'SV 4/15/2024 23:23'!usar 		durabilidad := durabilidad - 1.		(durabilidad <= 0) ifTrue: [ durabilidad := 0 ]. 		^durabilidad ! !Herramienta subclass: #Hacha	instanceVariableNames: ''	classVariableNames: ''	package: 'Herramientas'!!Hacha methodsFor: 'as yet unclassified' stamp: 'SV 4/15/2024 23:07'!templar	^HachaTemplada new.! !!Hacha methodsFor: 'as yet unclassified' stamp: 'SV 4/15/2024 23:09'!usar	self subclassResponsibility.! !Hacha subclass: #HachaNoTemplada	instanceVariableNames: ''	classVariableNames: ''	package: 'Herramientas'!!HachaNoTemplada methodsFor: 'as yet unclassified' stamp: 'SV 4/15/2024 23:09'!usar 	(durabilidad = 0) ifTrue: [ ^0 ]. 	durabilidad := durabilidad - 1.	^ durabilidad.! !Hacha subclass: #HachaTemplada	instanceVariableNames: ''	classVariableNames: ''	package: 'Herramientas'!!HachaTemplada methodsFor: 'as yet unclassified' stamp: 'SV 4/15/2024 23:08'!usar	^durabilidad! !Object subclass: #Herrero	instanceVariableNames: ''	classVariableNames: ''	package: 'Herramientas'!!Herrero methodsFor: 'as yet unclassified' stamp: 'SV 4/15/2024 23:05'!templar: unHacha 	^self crearPico. "unHacha templar"! !!Herrero methodsFor: 'as yet unclassified' stamp: 'SV 4/15/2024 22:42'!reparar: unaHerramienta 	unaHerramienta repararse.! !!Herrero methodsFor: 'as yet unclassified' stamp: 'SV 4/15/