Object subclass: #Aldeano	instanceVariableNames: 'durabilidadHacha durabilidadPico usosAzada durabilidadAzada'	classVariableNames: ''	package: 'Herramientas'!!Aldeano methodsFor: 'as yet unclassified' stamp: 'SV 4/15/2024 20:10'!serieFibonacciOrden: n 	| b a |	(n = 1) ifTrue: [^0].	(n <= 3) ifTrue: [^1].	a := b := 1.	(n - 2) timesRepeat: [a := b + (b := a)].	^b.! !!Aldeano methodsFor: 'as yet unclassified' stamp: 'SV 4/15/2024 19:38'!usarHacha	(durabilidadHacha = 0) ifTrue: [ ^0 ]. 	durabilidadHacha := durabilidadHacha - 1.	^ durabilidadHacha! !!Aldeano methodsFor: 'as yet unclassified' stamp: 'SV 4/15/2024 19:38'!usarPico	^durabilidadPico! !!Aldeano methodsFor: 'as yet unclassified' stamp: 'SV 4/15/2024 20:11'!usarAzada	| aRestar |	usosAzada := usosAzada + 1.	aRestar := self serieFibonacciOrden: usosAzada.	durabilidadAzada := durabilidadAzada - aRestar.	^durabilidadAzada ! !!Aldeano methodsFor: 'initialization' stamp: 'SV 4/15/2024 19:47'!initialize 	durabilidadHacha := 10.	durabilidadPico := 10.	durabilidadAzada := 10.	usosAzada := 0.! !