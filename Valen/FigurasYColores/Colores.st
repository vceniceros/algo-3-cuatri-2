PackageManifest subclass: #ManifestColores	instanceVariableNames: ''	classVariableNames: ''	package: 'Colores-Manifest'!!ManifestColores commentStamp: 'SV 4/9/2024 16:57' prior: 0!| unCompas unCirculo unaCartuchera unPincel unColor superficie|unCompas := Compas new.unCirculo := unCompas dibujarCirculoConRadio:5.unaCartuchera := Cartuchera new.unPincel := unaCartuchera getPinceles at:2.unColor := Rojo new.unPincel seleccionarColor: unColor.unPincel pintar: unCirculo.superficie := unCirculo calcularSuperficie. !Object subclass: #Cartuchera	instanceVariableNames: 'pinceles'	classVariableNames: ''	package: 'Colores'!!Cartuchera methodsFor: 'initialization' stamp: 'SV 4/9/2024 15:40'!initialize	pinceles := OrderedCollection new.	pinceles add: Fino new.	pinceles add: Grueso new.! !!Cartuchera methodsFor: 'as yet unclassified' stamp: 'SV 4/9/2024 15:11'!getPinceles	^ pinceles! !Object subclass: #Circulo	instanceVariableNames: 'radio relleno contorno'	classVariableNames: ''	package: 'Colores'!!Circulo methodsFor: 'as yet unclassified' stamp: 'SV 4/9/2024 15:23'!conRadio: unRadio 	radio := unRadio.! !!Circulo methodsFor: 'as yet unclassified' stamp: 'SV 4/9/2024 15:28'!calcularSuperficie	^ 	Float pi * radio ** 2! !!Circulo methodsFor: 'as yet unclassified' stamp: 'SV 4/9/2024 15:36'!contornoDe: unColor	contorno := unColor.! !!Circulo methodsFor: 'as yet unclassified' stamp: 'SV 4/9/2024 15:34'!rellenarCon: unColor 	relleno := unColor.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Circulo class	instanceVariableNames: ''!!Circulo class methodsFor: 'as yet unclassified' stamp: 'SV 4/9/2024 15:22'!conRadio: unRadio    | unCirculo |	unCirculo := self new.	unCirculo conRadio: unRadio.	^unCirculo! !Object subclass: #Compas	instanceVariableNames: ''	classVariableNames: ''	package: 'Colores'!!Compas methodsFor: 'as yet unclassified' stamp: 'SV 4/9/2024 15:21'!dibujarCirculoConRadio: unRadio 	^ Circulo conRadio: unRadio ! !Object subclass: #Pincel	instanceVariableNames: 'color'	classVariableNames: ''	package: 'Colores'!!Pincel methodsFor: 'as yet unclassified' stamp: 'SV 4/9/2024 15:16'!seleccionarColor: unColor 	color := unColor.! !!Pincel methodsFor: 'as yet unclassified' stamp: 'SV 4/9/2024 15:18'!pintar: unaFigura 	self subclassResponsibility! !Pincel subclass: #Fino	instanceVariableNames: ''	classVariableNames: ''	package: 'Colores'!!Fino methodsFor: 'as yet unclassified' stamp: 'SV 4/9/2024 15:42'!pintar: unaFigura	unaFigura contornoDe: color.! !Pincel subclass: #Grueso	instanceVariableNames: ''	classVariableNames: ''	package: 'Colores'!!Grueso methodsFor: 'as yet unclassified' stamp: 'SV 4/9/2024 15:42'!pintar: unaFigura 		unaFigura rellenarCon: color.! !Object subclass: #Rojo	instanceVariableNames: ''	classVariableNames: ''	package: 'Colores'!