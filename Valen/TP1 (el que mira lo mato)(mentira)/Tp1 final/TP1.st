Object subclass: #AlgoRastreadorZ	instanceVariableNames: 'identificarTransformacion seleccionarCriterio rastreos seleccionarRastreador'	classVariableNames: ''	package: 'TP1'!!AlgoRastreadorZ methodsFor: 'initialization' stamp: 'valentinoceniceros 5/9/2024 20:32'!initialize 	seleccionarRastreador := BuscarRastreador new .	identificarTransformacion := BuscarTransformacion new .	seleccionarCriterio := BuscarCriterio new.	rastreos := Criterio new.	! !!AlgoRastreadorZ methodsFor: 'not defined category' stamp: 'valentinoceniceros 5/9/2024 20:12'!nivelDePeleaDe: unPeleador	|kiRegistrado|	kiRegistrado  := rastreos obtenerLecturaPorNombre: unPeleador .		^kiRegistrado ! !!AlgoRastreadorZ methodsFor: 'not defined category' stamp: 'valentinoceniceros 4/18/2024 03:47'!conCriterioMas:  unCriterio	|algoRastreadorZ|	algoRastreadorZ := AlgoRastreadorZ new.	algoRastreadorZ criterio: unCriterio .	algoRastreadorZ ! !!AlgoRastreadorZ methodsFor: 'not defined category' stamp: 'valentinoceniceros 5/2/2024 18:51'!obtenerPeleadorSegunCriterio	|peleador|	peleador := rastreos obtenerPeleador.	^peleador darNombre! !!AlgoRastreadorZ methodsFor: 'as yet unclassified' stamp: 'valentinoceniceros 5/9/2024 20:32'!registrarRastreoConModelo: unModelo DelPeleadorConNombre: unNombre KiBase: unKi yTransformacion: unaTransformacion	|peleador  transformacion rastreadorUsado|		rastreadorUsado := seleccionarRastreador buscarPorClave: unModelo  .		transformacion := identificarTransformacion buscarPorClave: unaTransformacion  .		peleador := rastreadorUsado registrarRastroConNombre: unNombre  Ki: unKi  yTransformacion: transformacion.		rastreos agregarLectura: peleador	! !!AlgoRastreadorZ methodsFor: 'as yet unclassified' stamp: 'valentinoceniceros 4/19/2024 00:30'!criterio: unCriterio	rastreos := seleccionarCriterio buscarPorClave: unCriterio  .! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!AlgoRastreadorZ class	instanceVariableNames: ''!!AlgoRastreadorZ class methodsFor: 'not defined category' stamp: 'valentinoceniceros 4/5/2024 18:51'!conCriterioMas: unCriterio	|algoRastreadorZ|	algoRastreadorZ := AlgoRastreadorZ new.	algoRastreadorZ criterio: unCriterio .	^algoRastreadorZ ! !Object subclass: #BuscarEnDiccionario	instanceVariableNames: 'diccionario'	classVariableNames: ''	package: 'TP1'!!BuscarEnDiccionario methodsFor: 'initialization' stamp: 'valentinoceniceros 4/19/2024 00:13'!initialize 	diccionario := Dictionary new.! !!BuscarEnDiccionario methodsFor: 'not defined category' stamp: 'valentinoceniceros 4/19/2024 22:34'!buscarPorClave: unaClave		|elemento|	 elemento := diccionario  at: unaClave ifAbsent: [self error: 'Clave inexistente'].	^elemento ! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!BuscarEnDiccionario class	instanceVariableNames: 'diccionario'!!BuscarEnDiccionario class methodsFor: 'as yet unclassified' stamp: 'valentinoceniceros 4/18/2024 23:34'!buscarPorClave: unaClave	diccionario at: unaClave .! !!BuscarEnDiccionario class methodsFor: 'testing' stamp: 'valentinoceniceros 4/19/2024 00:33'!isAbstract	^ self == BuscarEnDiccionario! !BuscarEnDiccionario subclass: #BuscarCriterio	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1'!!BuscarCriterio methodsFor: 'initialization' stamp: 'valentinoceniceros 4/19/2024 00:10'!initialize 	super initialize .	diccionario at:'Fuerte' put: Fuerte new.	diccionario 	at:'Debil' put: Debil new. ! !BuscarEnDiccionario subclass: #BuscarRastreador	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1'!!BuscarRastreador methodsFor: 'initialization' stamp: 'valentinoceniceros 4/19/2024 00:12'!initialize 	super initialize .	diccionario at:'Viejo' put: Viejo new.	diccionario	at:'Nuevo' put: Nuevo new.! !BuscarEnDiccionario subclass: #BuscarTransformacion	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1'!!BuscarTransformacion methodsFor: 'initialization' stamp: 'valentinoceniceros 4/19/2024 00:11'!initialize 	super initialize .	diccionario 	at: 'Base' put: Base new.	diccionario 	at: 'Kaio-Ken' put: KaioKen new.	diccionario 	at: 'Mono Gigante' put: MonoGigante new.! !Object subclass: #Criterio	instanceVariableNames: 'lecturas'	classVariableNames: ''	package: 'TP1'!!Criterio methodsFor: 'as yet unclassified' stamp: 'valentinoceniceros 4/13/2024 18:00'!obtenerPeleador	self shouldBeImplemented ! !!Criterio methodsFor: 'as yet unclassified' stamp: 'valentinoceniceros 4/13/2024 18:02'!agregarLectura: unPeleador	lecturas add: unPeleador ! !!Criterio methodsFor: 'initialization' stamp: 'valentinoceniceros 4/13/2024 17:54'!initialize 	lecturas := OrderedCollection new.! !!Criterio methodsFor: 'not defined category' stamp: 'valentinoceniceros 4/19/2024 00:27'!obtenerLecturaPorNombre: unNombre	|peleador|	peleador := lecturas detect: [ :p | p verSiExistePeleador: unNombre ].	^peleador ki.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Criterio class	instanceVariableNames: ''!!Criterio class methodsFor: 'testing' stamp: 'valentinoceniceros 4/19/2024 00:33'!isAbstract	^ self == Criterio! !Criterio subclass: #Debil	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1'!!Debil methodsFor: 'not defined category' stamp: 'valentinoceniceros 4/16/2024 18:33'!obtenerPeleador 	 | minKi peleadorHallado|    minKi := lecturas min: [ :peleador | peleador ki ].    peleadorHallado := lecturas detect: [ :peleador | peleador ki = minKi ].	 ^peleadorHallado ! !Criterio subclass: #Fuerte	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1'!!Fuerte methodsFor: 'not defined category' stamp: 'valentinoceniceros 4/16/2024 18:33'!obtenerPeleador 	| maxKi peleadorHallado|    maxKi := lecturas max: [ :peleador | peleador ki ].    peleadorHallado := lecturas detect: [ :peleador | peleador ki = maxKi ].	 ^peleadorHallado  ! !Object subclass: #Peleador	instanceVariableNames: 'nombre ki transformacion'	classVariableNames: ''	package: 'TP1'!!Peleador methodsFor: 'not defined category' stamp: 'valentinoceniceros 5/9/2024 19:21'!ConNombre: unNombre Ki: unKi yTransformacion: unaTransformacion	|peleador|	peleador := Peleador new.	peleador nombre: unNombre .	peleador transformacion: unaTransformacion .	peleador ki: unKi .	peleador ! !!Peleador methodsFor: 'not defined category' stamp: 'valentinoceniceros 5/9/2024 00:20'!darNombre	^nombre ! !!Peleador methodsFor: 'not defined category' stamp: 'valentinoceniceros 5/9/2024 00:25'!verSiExistePeleador: unNombre	^ nombre = unNombre ! !!Peleador methodsFor: 'initialization' stamp: 'valentinoceniceros 5/9/2024 00:16'!initialize 	super.	nombre := ''.	ki := 0.	transformacion := Transformacion new.! !!Peleador methodsFor: 'accessing' stamp: 'valentinoceniceros 5/9/2024 00:19'!nombre: unNombre	nombre := unNombre ! !!Peleador methodsFor: 'accessing' stamp: 'valentinoceniceros 5/9/2024 00:20'!transformacion: unaTransformacion	transformacion := unaTransformacion ! !!Peleador methodsFor: 'accessing' stamp: 'valentinoceniceros 5/9/2024 19:36'!ki	^ transformacion transformarKi: ki! !!Peleador methodsFor: 'accessing' stamp: 'valentinoceniceros 5/9/2024 19:36'!ki: unKi	ki := unKi   .	(ki < 0) ifTrue: [self error: 'kiBase no puede ser menor que 0']! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Peleador class	instanceVariableNames: ''!!Peleador class methodsFor: 'not defined category' stamp: 'valentinoceniceros 5/9/2024 00:19'!ConNombre: unNombre Ki: unKi yTransformacion: unaTransformacion	|peleador|	peleador := Peleador new.	peleador nombre: unNombre .	peleador ki: unKi .	peleador transformacion: unaTransformacion .	^peleador ! !Object subclass: #Rastreador	instanceVariableNames: 'modelo'	classVariableNames: ''	package: 'TP1'!!Rastreador methodsFor: 'as yet unclassified' stamp: 'valentinoceniceros 5/9/2024 19:50'!registrarRastroConNombre: unNombre Ki: unKi  yTransformacion: unaTransformacion 	self shouldNotImplement  ! !!Rastreador methodsFor: 'as yet unclassified' stamp: 'valentinoceniceros 5/9/2024 19:25'!inicialize 	modelo := 'modelo'.! !!Rastreador methodsFor: 'not defined category' stamp: 'valentinoceniceros 5/9/2024 21:58'!calcularKi: unKi	self shouldNotImplement ! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Rastreador class	instanceVariableNames: ''!!Rastreador class methodsFor: 'testing' stamp: 'valentinoceniceros 4/19/2024 00:33'!isAbstract	^ self == Rastreador! !Rastreador subclass: #Nuevo	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1'!!Nuevo methodsFor: 'initialization' stamp: 'valentinoceniceros 5/9/2024 19:25'!inicialize 	modelo := 'Nuevo'.! !!Nuevo methodsFor: 'not defined category' stamp: 'valentinoceniceros 5/9/2024 19:51'!registrarRastroConNombre: unNombre Ki: unKi  yTransformacion: unaTransformacion | peleador kiRegistrado|	kiRegistrado := self calcularKi: unKi .	peleador := Peleador ConNombre: unNombre  Ki: kiRegistrado yTransformacion: unaTransformacion. ^peleador ! !!Nuevo methodsFor: 'not defined category' stamp: 'valentinoceniceros 5/9/2024 21:59'!calcularKi: unKi	^unKi ! !Object subclass: #Transformacion	instanceVariableNames: 'multiplicador'	classVariableNames: ''	package: 'TP1'!!Transformacion methodsFor: 'initialization' stamp: 'valentinoceniceros 4/13/2024 16:49'!initialize 	multiplicador := 0! !!Transformacion methodsFor: 'not defined category' stamp: 'valentinoceniceros 4/13/2024 16:51'!transformarKi: unKi	^unKi * multiplicador ! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Transformacion class	instanceVariableNames: ''!!Transformacion class methodsFor: 'testing' stamp: 'valentinoceniceros 4/19/2024 00:33'!isAbstract	^ self == Transformacion! !Transformacion subclass: #Base	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1'!!Base methodsFor: 'initialization' stamp: 'valentinoceniceros 4/13/2024 17:10'!initialize 	super initialize .	multiplicador := 1! !Transformacion subclass: #KaioKen	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1'!!KaioKen methodsFor: 'initialization' stamp: 'valentinoceniceros 4/13/2024 17:13'!initialize 	super initialize .	multiplicador := 2! !Transformacion subclass: #MonoGigante	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1'!!MonoGigante methodsFor: 'initialization' stamp: 'valentinoceniceros 4/17/2024 19:34'!transformarKi:  unKi	|ki|	ki :=  unKi * multiplicador .	^ ki asLargerPowerOfTwo ! !!MonoGigante methodsFor: 'initialization' stamp: 'valentinoceniceros 4/17/2024 19:21'!initialize 	super initialize .	multiplicador := 10! !Rastreador subclass: #Viejo	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1'!!Viejo methodsFor: 'as yet unclassified' stamp: 'valentinoceniceros 5/9/2024 19:25'!inicialize 	modelo := 'Viejo'.! !!Viejo methodsFor: 'not defined category' stamp: 'valentinoceniceros 5/9/2024 19:49'!registrarRastroConNombre: unNombre Ki: unKi  yTransformacion: unaTransformacion | peleador kiRegistrado kiTransformado|	kiTransformado := unaTransformacion transformarKi: unKi .	kiRegistrado :=  self calcularKi: kiTransformado .	peleador := Peleador ConNombre: unNombre  Ki: kiRegistrado yTransformacion: Base new .^peleador ! !!Viejo methodsFor: 'not defined category' stamp: 'valentinoceniceros 5/9/2024 19:43'!calcularKi: unKi	|limite |	limite := 9000.	^unKi min: limite  ! !